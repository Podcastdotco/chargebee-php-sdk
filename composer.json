{
  "name": "nathandunn/chargebee-php-sdk",
  "description": "PHP Client for using the Chargebee API.",
  "license": "MIT",
  "keywords": ["chargebee", "api-client"],
  "type": "library",
  "authors": [
    {
      "name": "Nathan Dunn",
      "email": "github@nathandunn.uk",
      "role": "Developer",
      "homepage": "https://nathandunn.uk"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "psr/http-message": "^1.0",
    "php-http/httplug": "^1.1",
    "php-http/message-factory": "^1.0",
    "php-http/discovery": "^1.1",
    "php-http/client-common": "^1.6",
    "php-http/multipart-stream-builder": "^1.0",
    "php-http/message": "^1.6",
    "symfony/options-resolver": "^2.6 || ^3.0 || ^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "php-http/mock-client": "^1.0",
    "php-coveralls/php-coveralls": "^2.0",
    "phpunit/phpcov": "^4.0",
    "php-http/guzzle6-adapter": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "NathanDunn\\Chargebee\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
